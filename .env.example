# ========== Volumes ==========
STRAPI_VOLUME=/path/to/strapi
POSTGRES_VOLUME=/path/to/data/postgresql
REDIS_VOLUME=/path/to/data/redis

# ========== Strapi Configuration ==========
NODE_ENV=development
STRAPI_PORT=1337
STRAPI_HOST=0.0.0.0

# ========== Redis Configuration ==========
REDIS_PORT=6379

# ========== PostgreSQL Configuration ==========
POSTGRES_USER=your_user
POSTGRES_PASSWORD=your_password
POSTGRES_DB=your_database

# ========== LiveKit ==========
# En local: IP de votre machine (ex: 192.168.1.21)
# En test: IP publique du VPS (ex: 195.35.0.168)
# En prod: IP publique du VPS (ex: 72.60.129.222)
LIVEKIT_EXTERNAL_IP=your_domain_or_ip
LIVEKIT_API_KEY=your_api_key
LIVEKIT_API_SECRET=your_secret_32_characters_long
LIVEKIT_URL=ws://your_domain_or_ip:7880
